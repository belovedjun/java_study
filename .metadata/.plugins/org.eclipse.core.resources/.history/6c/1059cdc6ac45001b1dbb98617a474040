public class VariableTypeMain02 {
	public static void main(String[] args ) {
		System.out.println("===정수형===");
		//byte, 크기 : 1byte, 표현범위 : -128~127
		byte b1 = 127;
		System.out.println("b1 = " + b1);
		
		//short, 크기 : 2byte, 표현범위 : -32,768 ~ 32,767
		short s1=32767;
		System.out.println("s1 = " + s1);
	}
}
